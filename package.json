    "@typescript-eslint/parser": "^5.11.0",
    "cdk8s-cli": "^2.0.83",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
